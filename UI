local function HHZCHN_fake_script() -- Set_WEBHOOK.ButtonScript 
	local script = Instance.new('LocalScript', Set_WEBHOOK)

	local Button = script["Parent"]
	local Webhook = Button:WaitForChild("WEBHOOK_VALUE")
	
	Button.MouseButton1Down:Connect(function(...)
		_G.Settings["WEBHOOK_VALUE"] = Webhook.Text;
		_G.SaveSettings();
		_G.TestWebhook()
		
		Webhook.Text = ""
	end)
end
coroutine.wrap(HHZCHN_fake_script)()
local function GWHXYTH_fake_script() -- Toggle_WEBHOOK.ButtonScript 
	local script = Instance.new('LocalScript', Toggle_WEBHOOK)

	local Button = script["Parent"]
	local SetButtonValue = function(Bool)
		if not Bool then
			Button:WaitForChild("Value").BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		elseif Bool then
			Button:WaitForChild("Value").BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		end
	end
	
	SetButtonValue(_G.GetSettings("WEBHOOK"))
	
	Button.MouseButton1Down:Connect(function(...)
		_G.Settings["WEBHOOK"] = not _G.Settings["WEBHOOK"]
		_G.SaveSettings()
	end)
end
coroutine.wrap(GWHXYTH_fake_script)()
local function OIFKDG_fake_script() -- UNLOAD.ButtonScript 
	local script = Instance.new('LocalScript', UNLOAD)

	local Button = script["Parent"]
	
	Button.MouseButton1Down:Connect(function(...)
		_G.UNLOADEDINF = true
		
		if game:GetService("CoreGui"):FindFirstChild("INF_LOGGER") then
			game:GetService("CoreGui"):FindFirstChild("INF_LOGGER"):Destroy()
		end
		
		game:GetService("CoreGui"):FindFirstChild("INF"):Destroy()
	end)
end
coroutine.wrap(OIFKDG_fake_script)()
