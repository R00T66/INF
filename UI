-- Scripts:

local function GVYW_fake_script() -- Button.ButtonScript 
	local script = Instance.new('LocalScript', Button)

	local Button = script["Parent"]
	local Frame = Button["Parent"]
	local GUI = Frame["Parent"]
	local Toggle = function()
		if GUI:FindFirstChild("Open") then
			if GUI:FindFirstChild("Open").Visible then
				Frame.Visible = false
				return
			else
				GUI:FindFirstChild("Open").Visible = true
				Frame.Visible = false
			end
		end
	end
	
	Button.MouseButton1Down:Connect(Toggle)
end
coroutine.wrap(GVYW_fake_script)()
local function MBTXP_fake_script() -- Button_2.ButtonScript 
	local script = Instance.new('LocalScript', Button_2)

	local Button = script["Parent"]
	local Title = Button["Parent"]
	local Frame = Title["Parent"]
	local GUI = Frame["Parent"]
	local Toggle = function()
		if GUI:FindFirstChild("Closed") then
			if GUI:FindFirstChild("Closed").Visible then
				Frame.Visible = false
				return
			else
				GUI:FindFirstChild("Closed").Visible = true
				Frame.Visible = false
			end
		end
	end
	
	Button.MouseButton1Down:Connect(Toggle)
end
coroutine.wrap(MBTXP_fake_script)()
local function PGQRAZP_fake_script() -- Toggle_ACCESSORY.ButtonScript 
	local script = Instance.new('LocalScript', Toggle_ACCESSORY)

	local Button = script["Parent"]
	local SetButtonValue = function(Bool)
		if not Bool then
			Button:WaitForChild("Value").BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		elseif Bool then
			Button:WaitForChild("Value").BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		end
	end
	
	SetButtonValue(_G.Settings["ACCESSORY"])
	
	Button.MouseButton1Down:Connect(function(...)
		_G.Settings["ACCESSORY"] = not _G.Settings["ACCESSORY"]
		_G.SaveSettings()
		
		SetButtonValue(_G.Settings["ACCESSORY"])
	end)
end
coroutine.wrap(PGQRAZP_fake_script)()
